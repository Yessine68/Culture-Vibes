<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="686.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MakeReservationController">
   <children>
      <HBox fx:id="HorizBox" prefHeight="341.0" prefWidth="600.0">
         <children>
            <TilePane fx:id="VoyCard" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="VoyImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <Label fx:id="Title" text="Label" />
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="daysLB" text="Label">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="locationLB" text="Label">
                           <HBox.margin>
                              <Insets left="100.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                     <TilePane.margin>
                        <Insets />
                     </TilePane.margin>
                  </HBox>
                  <Button mnemonicParsing="false" onAction="#MakeReservation" text="Make a Reservation" />
               </children>
            </TilePane>
         </children>
      </HBox>
   </children>
</AnchorPane>
